<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.adm.model.AdmVO" table="administrator" >
        
        <id name="adm_no" column="adm_no">
            <generator class="com.adm.model.ADMNOGenerator" />            
        </id>
        
        <property name="adm_id" column="adm_id" />
        <property name="adm_psw" column="adm_psw" />
        <property name="adm_createdate" column="adm_createdate" />
        <property name="adm_status" column="adm_status" />
        
    </class>
</hibernate-mapping>